on(IconNames.Sad)\n        basic.pause(3000)\n        // Return to \"No\" icon\n        basic.showIcon(IconNames.No)\n    } else {\n        // Broadcast the selected message\n        radio.sendValue(\"msg\", selectedMessage)\n        // Send sender's ID to prevent self-reception\n        radio.sendValue(\"id\", deviceID)\n        // Indicate message sent\n        basic.showIcon(IconNames.Happy)\n        basic.pause(3000)\n        // Restore the last selected message\n        showMessagePattern(selectedMessage)\n    }\n})\n// Receive and process incoming messages\nradio.onReceivedValue(function (name, value) {\n    if (name == \"id\" && value == deviceID) {\n        // Ignore own messages\n        return\n    }\n    if (name == \"msg\") {\n        lastReceivedMessage = value\n        // Show that a new message was received\n        showReceivedPattern()\n        basic.pause(3000)\n        // Display received message or \"No\" icon\n        showMessagePattern(lastReceivedMessage)\n    }\n})\nlet lastReceivedMessage = 0\nlet selectedMessage = 0\nlet deviceID = 0\n// Set group for communication\nradio.setGroup(1)\n// Allow time for radio setup\nbasic.pause(500)\n// Stores last received message\n// Unique ID for each Micro:bit\ndeviceID = control.deviceSerialNumber()\n// Show a tick icon to indicate successful initialization\nbasic.showIcon(IconNames.Yes)"],[0,"\n"]],"start1":0,"start2":0,"length1":3330,"length2":1}]},{"type":"edited","filename":"pxt.json","patch":[{"diffs":[[0,"   \""],[-1,"preferredEditor\": \"blocksprj\""],[1,"additionalFilePaths\": []"],[0,"\n}\n"]],"start1":232,"start2":232,"length1":36,"length2":31}]}]},{"timestamp":1741067908442,"editorVersion":"7.0.57","changes":[{"type":"edited","filename":"main.blocks","patch":[{"diffs":[[0," x=\""],[1,"2"],[0,"0\" y=\"33"],[-1,"1"],[1,"3"],[0,"5\" w"]],"start1":269,"start2":269,"length1":17,"length2":18},{"diffs":[[0,"art\" x=\""],[1,"2"],[0,"0\" y=\""],[1,"2"],[0,"0\"><stat"]],"start1":374,"start2":374,"lengt